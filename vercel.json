{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "bash avp.sh",
  "env": {
    "PYTHONPATH": "packages",
    "LD_LIBRARY_PATH": "/var/task/packages"
  },
  "functions": {
    "api/main.py": {
      "runtime": "python3.11",
      "maxDuration": 300,
      "memory": 2048,
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "api/main.py" }
  ]
}
