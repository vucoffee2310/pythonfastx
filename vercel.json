{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "bash avp.sh",
  "framework": "fastapi",
  "functions": {
    "main.py": {
      "maxDuration": 300,
      "runtime": "@vercel/python@6.1.5"
    },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
