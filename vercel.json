{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "bash avp.sh",
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@6.1.5",
      "maxDuration": 300,
      "memory": 2048
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "api/main.py" }
  ]
}
